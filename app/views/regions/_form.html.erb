

<%= form_for @region do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="form-group">
    <%= f.label :description %>
    <%= f.text :description %>
  </div>


  <div class="form-group">
    <%= f.submit %> 
  </div>          
<% end %>